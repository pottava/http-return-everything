{
	"name": "Go",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"forwardPorts": [8080],
	"runArgs": [ "--cap-add=SYS_PTRACE", "--security-opt", "seccomp=unconfined" ],
	"settings": {
		"terminal.integrated.shell.linux": "/bin/ash",
		"go.gopath": "/go",
		"go.useLanguageServer": true
	},
	"extensions": [
		"golang.go",
		"oderwat.indent-rainbow"
	],
	"remoteUser": "vscode"
}
